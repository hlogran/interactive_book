    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.3",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
